    "@types/node": "^20.8.0",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "@types/jest": "^29.5.0",
    "eslint": "^8.50.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
