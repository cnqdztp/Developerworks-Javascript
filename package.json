{
  "name": "developerworks-javascript-sdk",
  "version": "1.0.0",
  "description": "JavaScript/TypeScript SDK for DeveloperWorks AI services",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc",
